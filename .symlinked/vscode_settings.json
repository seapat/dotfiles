{
  "[typst]": {
    "editor.defaultFormatter": "nvarner.typst-lsp"
  },
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": "'cv02', 'ss02', 'ss05', 'ss03', 'cv19', 'cv24'",
  "editor.fontSize": 15,
  "editor.fontWeight": "400",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": true,
  "files.associations": {
    "*.c": "c",
    "*.cpp": "cpp",
    "*.h": "c",
    "*.hpp": "cpp",
    "*.inc": "c",
    "ranges": "c",
    "type_traits": "c",
    "utility": "c",
    "xstddef": "c"
  },
  "files.exclude": {},
  "git.autofetch": true,
  "git.confirmSync": false,
  "nix.enableLanguageServer": true,
  "nix.formatterPath": "alejandra",
  "nix.serverPath": "nil",
  "nix.serverSettings": {
    "nil": {
      "diagnostics": {
        "excludedFiles": [],
        "ignored": []
      },
      "nix": {
        "flake": {
          "autoArchive": false,
          "autoEvalInputs": false,
          "nixpkgsInputName": "nixpkgs"
        }
      }
    },
    "nixd": {
      "formatting": {
        "command": "alejandra"
      },
      "options.enable": true
    }
  },
  "notebook.stickyScroll.enabled": false,
  "notebook.stickyScroll.mode": "indented",
  "projectManager.git.baseFolders": [
    "/home/sean/Github"
  ],
  "terminal.integrated.mouseWheelZoom": true,
  "typst-lsp.experimentalFormatterMode": "on",
  "typst-lsp.exportPdf": "onPinnedMainSave",
  "update.mode": "none",
  "window.autoDetectColorScheme": true,
  "window.commandCenter": true,
  "window.menuBarVisibility": "classic",
  "window.systemColorTheme": true,
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "",
  "workbench.editor.empty.hint": "hidden",
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.preferredDarkColorTheme": "Default Dark Modern",
  "workbench.preferredLightColorTheme": "Default Light Modern",
  "workbench.tree.enableStickyScroll": true
}
